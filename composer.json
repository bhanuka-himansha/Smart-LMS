{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2.4",
        "bolechen/nova-activitylog": "^0.4.1",
        "creativeorange/gravatar": "~1.0",
        "customf/fabric": "@dev",
        "digital-creative/collapsible-resource-manager": "^2.1",
        "digital-creative/icon-action-toolbar": "^0.1.2",
        "digital-creative/nova-filepond": "^1.0",
        "fabricjs/canvasfield": "@dev",
        "guzzlehttp/guzzle": "^7.2",
        "jubaer/zoom-laravel": "^1.3",
        "laravel/framework": "^11.0",
        "laravel/nova": "*",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "league/flysystem-aws-s3-v3": "^3.15",
        "maatwebsite/excel": "^3.1",
        "outl1ne/nova-multiselect-field": "^4.5",
        "pktharindu/nova-permissions": "^3.0",
        "spatie/laravel-activitylog": "^4.7",
        "srmklive/paypal": "~3.0",
        "stripe/stripe-php": "^14.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "orangehill/iseed": "^3.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/CanvasField"
        },
        {
            "type": "path",
            "url": "./nova-components/Fabric"
        }
    ]
}
